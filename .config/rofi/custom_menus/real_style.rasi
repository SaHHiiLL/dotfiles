
@import                          "colors.rasi"
@import                          "fonts.rasi"

* {
  background-color: @background;
  text-color: @text;
}

window {
  height: 75px;
  width: 1000px;

  border-radius: 20px;
  padding: 20px;
  border: 3px solid;
  border-radius:  20px;
  border-color: @selected;
}

mainbox {
  enabled: true;
  background-color: @background;
}
 
prompt { 
  enabled: true;
  padding: 0 20 0 0;
}

